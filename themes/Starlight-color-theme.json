{
	"name": "Starlight",
	"type": "dark",
	"colors": {
		"editor.background": "#161928",
		"editor.foreground": "#e4edff",
		"activityBarBadge.background": "#007acc",
		"activityBar.background": "#0E0E11",
		"activityBar.foreground": "#98bcff",

		"titleBar.activeBackground": "#0E0E11",

		"sideBarTitle.foreground": "#bbbbbb",
